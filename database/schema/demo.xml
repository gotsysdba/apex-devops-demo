<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog 
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:n0="http://www.oracle.com/xml/ns/dbchangelog-ext" 
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog 
	http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
	<changeSet id="Initial" author="john" failOnError="true"    >
		<n0:runOracleScript objectName="SCRIPT" objectType="APEX" ownerName="ADMIN" sourceType="STRING"  >
			<n0:source><![CDATA[
	CREATE USER DEMO IDENTIFIED BY "${password}"
       DEFAULT COLLATION "USING_NLS_COMP" 
       DEFAULT TABLESPACE "DATA"
       TEMPORARY TABLESPACE "TEMP";
]]></n0:source>
		</n0:runOracleScript>
		<rollback>drop user DEMO cascade;</rollback>
	</changeSet>
</databaseChangeLog>